<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="ColorPrimary1">#3F51B5</Color>
    <Color x:Key="ColorPrimary2">#303F9F</Color>

    <SolidColorBrush x:Key="ColorPrimary1Brush" Color="{StaticResource ColorPrimary1}" />
    <SolidColorBrush x:Key="ColorPrimary2Brush" Color="{StaticResource ColorPrimary2}" />
    
</ResourceDictionary>